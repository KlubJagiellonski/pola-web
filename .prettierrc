{
  "semi": true,
  "singleQuote": true,
  "trailingComma": "es5",
  "printWidth": 120,
  "tabWidth": 2,
  "jsxBracketSameLine": true,
  "importOrder": [
    "^(react|gatsby)(.*)$",
    "^-[./]$",
    "^-@[-(.|..)[./]",
    "^-@[(.|..)[./]",
    "^@[./]",
    "^@(App|Domain|State)/(.*)$",
    "^@(Layout|Components|Utils)/(.*)$",
    "^[./]",
    "^@Styles/(.*)$",
    "^@Assets/(.*)$"
  ],
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": true,
  "organizeImportsSkipDestructiveCodeActions": true
}
